# MQTT Broker Configuration
MQTT_BROKER_URL=mqtt://localhost:1883
MQTT_CLIENT_ID=data-collector
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_TOPIC_PATTERN=data/+/+/+/+/+

# MongoDB Configuration
MONGODB_URI=mongodb://localhost:27017
MONGODB_MAX_POOL_SIZE=50
MONGODB_MIN_POOL_SIZE=10

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Queue Configuration
QUEUE_REALTIME_NAME=queue:realtime
QUEUE_HISTORY_NAME=queue:history
QUEUE_DEAD_LETTER_NAME=queue:dead_letter

# Worker Configuration
WORKER_HISTORY_INSTANCES=4
WORKER_REALTIME_INSTANCES=2
BATCH_SIZE_HISTORY=100
BATCH_SIZE_REALTIME=50
BATCH_INTERVAL_MS=5000

# Rate Limiting Configuration
RATE_LIMIT_MAX_CONCURRENT=10
RATE_LIMIT_MIN_TIME_MS=100
RATE_LIMIT_RESERVOIR=1000
RATE_LIMIT_RESERVOIR_REFRESH_INTERVAL_MS=60000

# Monitoring Configuration
MONITOR_INTERVAL_MS=30000
QUEUE_ALERT_THRESHOLD_HISTORY=50000
QUEUE_ALERT_THRESHOLD_REALTIME=10000

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=./logs/app.log

# Message Formatting Configuration
# Comma-separated list of prefixes to remove from message keys
MESSAGE_KEY_PREFIXES=INV_1_,INV_2_,INV_3_,INV_4_,CHINT_1_,CHINT_2_,CHINT_3_,CHINT_4_

# Dashboard Configuration
DASHBOARD_PORT=3000
